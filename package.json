{
  "name": "pixel-art-react",
  "version": "3.7.0",
  "description": "Draw and animate Pixel Art, then export the results to CSS, share or download them, powered by React",
  "author": "Javier Valencia Romero",
  "scripts": {
    "lint": "eslint **/*.{js,jsx}",
    "csslint": "stylelint src/**/*.{css,js,jsx}",
    "prettier": "prettier **/*.{js,jsx,json,yml,yaml,css,md}",
    "format": "npm run prettier -- --write --list-different",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "validate": "npm run lint && npm run csslint && npm run prettier -- --list-different && npm run test",
    "precommit": "lint-staged",
    "development": "webpack-dev-server --hot",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "server": "node ./src/server/loader",
    "server-debug": "node --inspect ./src/server/loader",
    "start": "npm run server",
    "postinstall": "NODE_ENV=production webpack -p --config webpack.production.config.js"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jvalen/pixel-art-react"
  },
  "keywords": [
    "pixel",
    "art",
    "drawing",
    "react",
    "redux",
    "express"
  ],
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^8.4.1",
    "blob-stream": "^0.1.3",
    "body-parser": "^1.18.2",
    "box-shadow-pixels": "0.0.3",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "file-loader": "^1.1.11",
    "gif-encoder": "^0.6.1",
    "gm": "^1.23.1",
    "html2canvas": "^1.0.0-alpha.12",
    "immutable": "^3.8.2",
    "jspdf": "^1.5.3",
    "lost": "^8.2.1",
    "oauth": "^0.9.15",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.4",
    "postcss-reporter": "^5.0.0",
    "precss": "^2.0.0",
    "pug": "^2.0.3",
    "radium": "^0.24.0",
    "react": "^16.3.2",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-beautiful-dnd": "^9.0.2",
    "react-color": "^2.14.1",
    "react-cookie-banner": "3.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.3.2",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-transform-hmr": "^1.0.4",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-undo": "^1.0.0-beta9",
    "shortid": "^2.2.8",
    "style-loader": "^0.21.0",
    "temp": "^0.8.3",
    "twitter": "^1.7.1",
    "url-loader": "^1.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "6.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^22.4.3",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "stylelint": "^9.9.0",
    "stylelint-config-lost": "0.0.3",
    "stylelint-config-recommended": "^2.1.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.22.1"
  }
}
